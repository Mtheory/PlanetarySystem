CREATE TABLE PLANET (ID NUMERIC PRIMARY KEY, DATEID TIMESTAMP)
CREATE TABLE MOON (ID NUMERIC PRIMARY KEY, DATEID TIMESTAMP)

CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PLANET(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(50),OU VARCHAR(40) NOT NULL,TITLE VARCHAR(10),FN VARCHAR(10) NOT NULL,SN VARCHAR(10) NOT NULL,MAIL VARCHAR(100),ROLE VARCHAR(5) NOT NULL,MEMBERTYPE VARCHAR(6) NOT NULL,GENDER VARCHAR(1) NOT NULL,USERPASSWORD VARCHAR(40),CONSTRAINT SYS_CT_64 UNIQUE(CN,OU))
CREATE MEMORY TABLE MOON (ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,CN VARCHAR(40) NOT NULL,OU VARCHAR(40) NOT NULL,TITLE VARCHAR(10),FN VARCHAR(10) NOT NULL,SN VARCHAR(10) NOT NULL,MAIL VARCHAR(100),ROLE VARCHAR(5) NOT NULL,MEMBERTYPE VARCHAR(6) NOT NULL,GENDER VARCHAR(1) NOT NULL,USERPASSWORD VARCHAR(40),CONSTRAINT SYS_CT_64 UNIQUE(CN,OU))
ALTER TABLE V_TABLE_USER ALTER COLUMN ID RESTART WITH 5
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
